{
  "name": "mongoose-data-seed",
  "version": "1.0.6",
  "description": "Seed mongodb with data using mongoose models",
  "main": "index.js",
  "scripts": {
    "build": "yarn build:clean && yarn build:dist",
    "build:clean": "rimraf dist",
    "build:dist": "babel src/ --out-dir dist/ --ignore 'src/**/*.spec.js'",
    "test": "cross-env NODE_ENV=test nyc --no-cache ava",
    "test:watch": "yarn test --watch",
    "test:check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint --max-warnings 0 .",
    "slate": "rimraf node_modules && yarn",
    "prepare": "yarn build"
  },
  "bin": {
    "md-seed": "./dist/bin/index.js"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "ava": {
    "files": [
      "src/lib/**/*.spec.js"
    ],
    "source": [
      "src/lib/**/*.js"
    ],
    "failFast": true,
    "babel": "inherit",
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "**/*.spec.js",
      "src/lib/config.js",
      "src/lib/utils/setup-test-env.js"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sharvit/mongoose-data-seed.git"
  },
  "keywords": [
    "mongoose",
    "seed",
    "data",
    "mongodb",
    "seeder",
    "mongoosejs"
  ],
  "author": "Avi Sharvit <sharvita@gmail.com> (https://sharvit.github.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sharvit/mongoose-data-seed/issues"
  },
  "homepage": "https://github.com/sharvit/mongoose-data-seed#readme",
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "chalk": "^2.0.1",
    "clui": "^0.3.1",
    "command-line-args": "^4.0.2",
    "command-line-commands": "^2.0.1",
    "command-line-usage": "^4.0.0",
    "find-root": "^1.0.0",
    "inquirer": "^4.0.1",
    "lodash": "^4.17.4",
    "log-symbols": "^2.0.0",
    "mem-fs": "^1.1.3",
    "mem-fs-editor": "^3.0.2",
    "rxjs": "^5.3.0"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.0.0",
    "babel-eslint": "^8.1.2",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-rewire": "^1.1.0",
    "babel-preset-env": "^1.6.1",
    "babel-template": "^6.2.0",
    "babel-types": "^6.2.0",
    "coveralls": "^3.0.0",
    "cross-env": "^5.0.1",
    "eslint": "^4.3.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-promise": "^3.6.0",
    "nyc": "^11.0.3",
    "pre-commit": "^1.2.2",
    "prettier": "^1.9.2",
    "rimraf": "^2.6.1",
    "sinon": "^4.1.3"
  }
}
