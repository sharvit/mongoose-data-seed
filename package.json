{
  "name": "mongoose-data-seed",
  "version": "1.0.7",
  "description": "Seed mongodb with data using mongoose models",
  "main": "index.js",
  "scripts": {
    "build": "yarn build:clean && yarn build:dist",
    "build:clean": "rimraf dist",
    "build:dist": "babel src/ --out-dir dist/ --ignore 'src/**/*.test.js','src/e2e/*','src/**/__mocks__/*','src/lib/utils/test-helpers.js','src/setup-test-env.js'",
    "test": "cross-env NODE_ENV=test nyc --no-cache ava",
    "test:watch": "yarn test --watch",
    "test:check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint --max-warnings 0 .",
    "slate": "rimraf node_modules && yarn",
    "prepare": "yarn build"
  },
  "bin": {
    "md-seed": "./md-seed.js"
  },
  "pre-commit": [
    "lint"
  ],
  "ava": {
    "concurrency": 5,
    "verbose": true,
    "failFast": false,
    "files": [
      "src/e2e/*.e2e.js",
      "src/lib/**/*.test.js"
    ],
    "sources": [
      "src/**/*.js"
    ],
    "require": [
      "./src/setup-test-env.js"
    ]
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "**/*.test.js",
      "**/__mocks__/**/*",
      "src/e2e/**/*",
      "src/lib/config.js",
      "src/lib/utils/test-helpers.js"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sharvit/mongoose-data-seed.git"
  },
  "keywords": [
    "mongoose",
    "seed",
    "data",
    "mongodb",
    "seeder",
    "mongoosejs"
  ],
  "author": "Avi Sharvit <sharvita@gmail.com> (https://sharvit.github.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sharvit/mongoose-data-seed/issues"
  },
  "homepage": "https://github.com/sharvit/mongoose-data-seed#readme",
  "dependencies": {
    "@babel/register": "^7.4.0",
    "@babel/runtime": "^7.4.0",
    "chalk": "^2.0.1",
    "clui": "^0.3.1",
    "command-line-args": "^5.0.2",
    "command-line-commands": "^2.0.1",
    "command-line-usage": "^5.0.5",
    "core-js": "^3.1.4",
    "find-root": "^1.0.0",
    "inquirer": "^6.2.0",
    "lodash": "^4.17.11",
    "log-symbols": "^3.0.0",
    "mem-fs": "^1.1.3",
    "mem-fs-editor": "^6.0.0",
    "regenerator-runtime": "^0.13.2",
    "rxjs": "^6.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.0",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/preset-env": "^7.4.0",
    "@babel/template": "^7.4.0",
    "@babel/types": "^7.4.0",
    "ava": "^2.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-plugin-rewire": "^1.2.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^6.0.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^9.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "mongoose": "^5.3.11",
    "ncp": "^2.0.0",
    "nyc": "^14.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.15.1",
    "prettier-eslint": "^9.0.0",
    "rimraf": "^2.6.1",
    "sinon": "^7.1.1"
  }
}
